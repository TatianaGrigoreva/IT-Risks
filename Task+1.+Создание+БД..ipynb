   },
   "outputs": [],
   "source": [
    "#настройка соединения с DB\n",
    "\n",
    "from sqlalchemy import create_engine\n",
    "engine = create_engine('postgresql+psycopg2://postgres:pass@localhost:5432/postgres')\n",
    "connection = engine.connect() \n"
   ]
